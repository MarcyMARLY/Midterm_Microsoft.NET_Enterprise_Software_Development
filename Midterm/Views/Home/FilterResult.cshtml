@model IEnumerable<Midterm.Models.Order>
@{
	ViewData["Title"] = "FilterResult";
}

<h2>FilterResult</h2>
<div>
	<table class="table">
		<thead>
			<tr>

				<th scope="row">Route</th>
				<th scope="row">Category</th>
				<th scope="row">Amount</th>
				<th scope="row">Status</th>
				<th scope="row">Date</th>
\



			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{

				<tr>

					<td>
						@item.route
					</td>
					<td>
						@item.category
					</td>
					<td>
						@item.amount
					</td>
					<td>
						@item.status
					</td>
					<td>
						@item.date
					</td>
					
				</tr>
			}
		</tbody>
	</table>

</div>



